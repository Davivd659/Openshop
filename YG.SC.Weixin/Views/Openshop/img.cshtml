					</p>
				</div>
			</figure>
		</div>

		<nav>
			<ul class="find-company-nav clearfix">
				<li style="width:49.5%"><a class="click-point" data-point="2_5_3_1" href="/Openshop/Openshop?id=@Model.Id"\">企业介绍</a>
				</li>
				<li style="width:49.5%"><a class="active">服务案例</a>
				</li>
			</ul>
		</nav>


		<ul class="dsger-productions">
            @if (Model.OpenShopPhoto != null)
                {
                    foreach (var item in Model.OpenShopPhoto)
                    {
			        <li>
				        <figure>
					
						        <img style="display: block;" src="@string.Format("http://www.kaidianing.com{0}",item.PhotoSmall)" class="lazy" data-original="@string.Format("http://www.kaidianing.com{0}",item.PhotoSmall)"
						        alt="@Model.Name"  width="600px" height="340px" onerror="javascript:this.src='/img/icon/pinpaiimg.jpg'">

					        <figcaption>
						        <h4>@Model.Name</h4>
						        <!--<ul>
							        <li>现代</li>
							        <li>80平方</li>
							        <li></li>
							        <li>3-5万</li>
						        </ul>-->
					        </figcaption>
				        </figure>
			        </li>
                     }
                }
                
			

		</ul>
